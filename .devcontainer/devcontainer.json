{
  "name": "SmartHome-Demo",
  "image": "mcr.microsoft.com/devcontainers/java:17",
  "features": {
    "ghcr.io/devcontainers/features/maven:1": {}
  },
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "visibility": "public",
      "protocol": "https",
      "onAutoForward": "notify"
    }
  },
  "postCreateCommand": "mvn clean package -DskipTests"
}


